<h1>PILL CREATE</h1>
<div class=“container”>
<form id="form" action="/pill/create" method="POST">
  <label>Title</label><br>
  <input type="text" name="title" placeholder="Title">
  <br><br>
  <label>Description</label><br>
  <input type="text" name="description" placeholder="Description">
  <br><br>
  <label>Link</label><br>
  <input type="text" name="link" placeholder="Youtube link">
  <br><br>
  <label>Category</label><br>
  <select name="language">
   <option value="1">AJAX</option>
   <option value="2">Arrays</option>
   <option value="3">Async & Callbacks</option>
   <option value="4">Algorithms</option>
   <option value="5">Canvas</option>
   <option value="6">Conditionals & Loops</option>
   <option value="7">Debugging</option>
   <option value="8">DOM</option>
   <option value="9">Express</option>
   <option value="10">Functions</option>
   <option value="11">JQuery</option>
   <option value="12">JSON</option>
   <option value="13">Libraries</option>
   <option value="14">Methods</option>
   <option value="15">Mongo</option>
   <option value="16">Mongoose</option>
   <option value="17">Node</option>
   <option value="18">Objects</option>
   <option value="19">Packages</option>
   <option value="20">Prototype</option>
   <option value="21">Passport</option>
   <option value="22">Testing</option>
  </select>
  <br><br>
  <label>Language</label><br>
  <select name="language">
   <option value="1">English</option>
   <option value="2">Spanish</option>
  </select>
  <!-- <label>Language</label><br>
  <input type="text" name="language" placeholder="Choose language"> -->
  <br><br>
  <div class=“container”>
  <div class=“center-btn right”>
    <button class="btn waves-effect waves-light" type="submit" name="action">CREATE PILL
  </button>
   <br>
 </form>
  </div>
