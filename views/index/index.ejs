<h1>hola</h1>
<div class="row">

    <% pills.forEach(pill => { %>
      <div class="col s12 m6">
        <div class="card hoverable">
          <div class="card-image">
            <a href="/pills/<%= pill._id %>/detail" class="btn-floating halfway-fab waves-effect waves-light red"></a>
          </div>
          <div class="card-content">
            <br>
            <p><b><%= pill.title %></b></p>
            <p>
              <%= pill.link %>
            </p>
            <p>
              <%= pill.description %>
            </p>
            <p>
              <%= pill.category %>
            </p>
            <p>
              <%= pill.language %>
            </p>
          </div>
        </div>
      </div>
      <% }) %>
  
</div>
</div>
